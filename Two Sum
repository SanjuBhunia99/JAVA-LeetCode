class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] result = new int[2];//{0,1}
        for(int i= 0;i<nums.length;i++){//i = 0
            for(int j = 0; j < nums.length;j++){//j = 1
                if(i != j&& nums[i] + nums[j] == target){//i!=j num[0](2)+num[1](7)==target (9)
                    result[1] = i;
                    result[0] = j;
                    break;
                }
            }
            
        }
        return result;
    }
}
