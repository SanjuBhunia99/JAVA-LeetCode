class Solution {
    public int[] minBitwiseArray(List<Integer> nums) {
        int n = nums.size();
        int[] ans = new int[n];

        for (int i = 0; i < n; i++) {
            int num = nums.get(i);
            int x = -1;

            for (int candidate = 0; candidate < num; candidate++) {
                if ((candidate | (candidate + 1)) == num) {
                    x = candidate;
                    break;
                }
            }

            ans[i] = x;
        }

        return ans;
    }
}
