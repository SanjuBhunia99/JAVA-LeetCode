class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int count = 0;
        int maximum = 0;
        for(int i = 0; i< nums.length; i++) {
            if(nums[i] == 1) {
                count = count + 1;
            }
            else {
                maximum = max(maximum, count);
                count = 0;
            }
        }
        
        return max(maximum, count);
    }
    
    public int max(int first, int second) {
        if (first > second) {
            return first;
        }
        return second;
    }
}
